                    "Destination": "Parameters"
                },
                {
                    "Name": "parVirtualWanHubs.value.0.parHubLocation",
                    "Destination": "Parameters"
                },
                {
                    "Name": "LOCATION",
                    "Destination": "Environment"
                }
            ],
            "AllowedValues": {
                "Display": false,
                "Values": [
